<?xml version="1.0" encoding="UTF-8"?><activity templateid="MCQMSP" activityid="" casecheck="yes" defaultscore="1" generatorid="PageSeeder" datetimestamp=""><directions><content type="html" tag="directions"><![CDATA[ Click on the open book icon in the circle to read and listen to the reading passage. Click all the correct answers for each question. Click <b>Submit</b> to check your answers. ]]></content></directions><questionscommon><content type="external" tag="commonPDF">activities/resources/externals/L2-U05-LB-RC.pdf</content></questionscommon><questionsets><questionset><question><content type="text" tag="questionText">What are other names for tropical cyclones?</content></question><options><option><content type="text" tag="optionText">tornadoes</content></option><option><content type="text" tag="optionText">hurricanes</content></option><option><content type="text" tag="optionText">typhoons</content></option><option><content type="text" tag="optionText">thunder storms</content></option></options><answers><answer><content type="number" tag="answerIndex">2</content></answer><answer><content type="number" tag="answerIndex">3</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">Where do tropical cyclones occur?</content></question><options><option><content type="text" tag="optionText">near the equator</content></option><option><content type="text" tag="optionText">27 degrees north of the equator</content></option><option><content type="text" tag="optionText">over water warmer than 27 degrees Celsius</content></option><option><content type="text" tag="optionText">over high waves in the ocean</content></option></options><answers><answer><content type="number" tag="answerIndex">1</content></answer><answer><content type="number" tag="answerIndex">3</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">What usually kills people during a tropical cyclone?</content></question><options><option><content type="text" tag="optionText">very strong winds</content></option><option><content type="text" tag="optionText">heavy rains</content></option><option><content type="text" tag="optionText">storm surges</content></option><option><content type="text" tag="optionText">a rise in sea level</content></option></options><answers><answer><content type="number" tag="answerIndex">3</content></answer><answer><content type="number" tag="answerIndex">4</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">In paragraph 4, what does "to predict" mean?</content></question><options><option><content type="text" tag="optionText">to study</content></option><option><content type="text" tag="optionText">to say what will happen in the future</content></option><option><content type="text" tag="optionText">to measure</content></option><option><content type="text" tag="optionText">to tell or describe something before it happens </content></option></options><answers><answer><content type="number" tag="answerIndex">2</content></answer><answer><content type="number" tag="answerIndex">4</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">What makes a tropical cyclone weaker?</content></question><options><option><content type="text" tag="optionText">large ocean waves</content></option><option><content type="text" tag="optionText">warm, deep ocean waters</content></option><option><content type="text" tag="optionText">moving over land</content></option><option><content type="text" tag="optionText">the earth's rotation</content></option></options><answers><answer><content type="number" tag="answerIndex">1</content></answer><answer><content type="number" tag="answerIndex">3</content></answer></answers></questionset></questionsets></activity>
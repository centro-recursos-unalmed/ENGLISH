<?xml version="1.0" encoding="UTF-8"?><activity templateid="MCQMSP" activityid="" casecheck="yes" defaultscore="1" generatorid="PageSeeder" datetimestamp=""><directions><content type="html" tag="directions"><![CDATA[ Click on the open book icon in the circle to read and listen to the reading passage. Click all the correct answers for each question. Click <b>Submit</b> to check your answers. ]]></content></directions><questionscommon><content type="external" tag="commonPDF">activities/resources/externals/L1-U10-LA-RC.pdf</content></questionscommon><questionsets><questionset><question><content type="text" tag="questionText">Which of these are mysteries about Teotihuacan?</content></question><options><option><content type="text" tag="optionText">how people dressed</content></option><option><content type="text" tag="optionText">how people lived</content></option><option><content type="text" tag="optionText">who is buried in the Pyramid of the Moon</content></option><option><content type="text" tag="optionText">why people abandoned the city</content></option></options><answers><answer><content type="number" tag="answerIndex">2</content></answer><answer><content type="number" tag="answerIndex">4</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">What did archeologists discover in the Pyramid of the Moon?</content></question><options><option><content type="text" tag="optionText">pottery</content></option><option><content type="text" tag="optionText">animals</content></option><option><content type="text" tag="optionText">headless bodies</content></option><option><content type="text" tag="optionText">a king</content></option></options><answers><answer><content type="number" tag="answerIndex">2</content></answer><answer><content type="number" tag="answerIndex">3</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">Why was Teotihuacan probably NOT a peaceful society?</content></question><options><option><content type="text" tag="optionText">because people were buried with animals</content></option><option><content type="text" tag="optionText">because people were buried with statues</content></option><option><content type="text" tag="optionText">because people's heads were cut off</content></option><option><content type="text" tag="optionText">because people were buried alive</content></option></options><answers><answer><content type="number" tag="answerIndex">3</content></answer><answer><content type="number" tag="answerIndex">4</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">In line 25, what does "in reality" mean?</content></question><options><option><content type="text" tag="optionText">In fact</content></option><option><content type="text" tag="optionText">But</content></option><option><content type="text" tag="optionText">Because</content></option><option><content type="text" tag="optionText">The truth is</content></option></options><answers><answer><content type="number" tag="answerIndex">1</content></answer><answer><content type="number" tag="answerIndex">4</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">Which of these statements are true about the people of Teotihuacan?</content></question><options><option><content type="text" tag="optionText">They were very peaceful.</content></option><option><content type="text" tag="optionText">They had weapons.</content></option><option><content type="text" tag="optionText">They had gentle leaders.</content></option><option><content type="text" tag="optionText">They probably had an army.</content></option></options><answers><answer><content type="number" tag="answerIndex">2</content></answer><answer><content type="number" tag="answerIndex">4</content></answer></answers></questionset></questionsets></activity>
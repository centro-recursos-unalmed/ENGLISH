<?xml version="1.0" encoding="UTF-8"?><activity templateid="MCQMSP" activityid="" casecheck="yes" defaultscore="1" generatorid="PageSeeder" datetimestamp=""><directions><content type="html" tag="directions"><![CDATA[ Click on the open book icon in the circle to read and listen to the reading passage. Click all the correct answers for each question. Click <b>Submit</b> to check your answers. ]]></content></directions><questionscommon><content type="external" tag="commonPDF">activities/resources/externals/L1-U01-LA-RC.pdf</content></questionscommon><questionsets><questionset><question><content type="text" tag="questionText">How do dolphins communicate?</content></question><options><option><content type="text" tag="optionText">They whistle.</content></option><option><content type="text" tag="optionText">They use sounds.</content></option><option><content type="text" tag="optionText">They use nets.</content></option><option><content type="text" tag="optionText">They use body language.</content></option></options><answers><answer><content type="number" tag="answerIndex">1</content></answer><answer><content type="number" tag="answerIndex">2</content></answer><answer><content type="number" tag="answerIndex">4</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">Which sentences are true?</content></question><options><option><content type="text" tag="optionText">Dolphins live alone.</content></option><option><content type="text" tag="optionText">Dolphins like to play.</content></option><option><content type="text" tag="optionText">Dolphins have names.</content></option><option><content type="text" tag="optionText">Dolphins live for 40 years.</content></option></options><answers><answer><content type="number" tag="answerIndex">2</content></answer><answer><content type="number" tag="answerIndex">3</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">How do we know that dolphins are intelligent?</content></question><options><option><content type="text" tag="optionText">because they live in pods</content></option><option><content type="text" tag="optionText">because they talk to each other</content></option><option><content type="text" tag="optionText">because they can catch fish with nets</content></option><option><content type="text" tag="optionText">because they play together</content></option></options><answers><answer><content type="number" tag="answerIndex">2</content></answer><answer><content type="number" tag="answerIndex">4</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">How are dolphins similar to people?</content></question><options><option><content type="text" tag="optionText">They use nets to catch fish.</content></option><option><content type="text" tag="optionText">They communicate with each other.</content></option><option><content type="text" tag="optionText">They like warm water.</content></option><option><content type="text" tag="optionText">They make plans.</content></option></options><answers><answer><content type="number" tag="answerIndex">2</content></answer><answer><content type="number" tag="answerIndex">4</content></answer></answers></questionset><questionset><question><content type="text" tag="questionText">Why do dolphins signal to fishermen?</content></question><options><option><content type="text" tag="optionText">so the fishermen will know that there are fish in the water</content></option><option><content type="text" tag="optionText">so the fishermen will throw their nets in the water</content></option><option><content type="text" tag="optionText">so the fishermen will throw them food</content></option><option><content type="text" tag="optionText">to say hello to the fishermen</content></option></options><answers><answer><content type="number" tag="answerIndex">1</content></answer><answer><content type="number" tag="answerIndex">2</content></answer></answers></questionset></questionsets></activity>